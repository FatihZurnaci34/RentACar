    <script src="/temp/js/popper.min.js"></script>
    <script src="/temp/js/bootstrap.min.js"></script>
    <script src="/temp/js/jquery.easing.1.3.js"></script>
    <script src="/temp/js/jquery.waypoints.min.js"></script>
    <script src="/temp/js/jquery.stellar.min.js"></script>
    <script src="/temp/js/owl.carousel.min.js"></script>
    <script src="/temp/js/jquery.magnific-popup.min.js"></script>
    <script src="/temp/js/aos.js"></script>
    <script src="/temp/js/jquery.animateNumber.min.js"></script>
